pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Build') {
      steps {
        sh 'mvn clean install site surefire-report:report'
        sh 'tree'
      }
    }
stage('Quality Gate') {
      steps {
            timeout(time: 2, unit: 'MINUTES'){
            waiteForQualityGate abortPipeline: true
            }
      }
    }
  }

}
